<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net6.0;netstandard2.1;</TargetFrameworks>
		<LangVersion>10</LangVersion>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<PackageId>ForRangeLoop</PackageId>
		<Title>ForRangeLoop</Title>
		<Version>1.0.0</Version>
		<Authors>Ziaul Hasan Hamim</Authors>
		<Description>For range loop(like python and kotlin) support in C#</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageTags>loop;range;forloop;</PackageTags>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<RepositoryUrl>https://github.com/ziaulhasanhamim/ForRangeLoop</RepositoryUrl>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<NoWarn>1591;1573</NoWarn>
	</PropertyGroup>
	
</Project>